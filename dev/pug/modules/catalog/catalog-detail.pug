                                .catalog__item-subtitle
                                    p Оцинкованная труба 20х20, СПК стандарт 4мм
                            .catalog__item-price
                                .actual-price
                                    p от <span>10000</span> руб.
                                .old-price
                                    p от 15000 руб.
                            .catalog__item-order(data-item-link="https://www.meduza-web.ru/")
